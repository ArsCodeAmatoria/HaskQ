{
  "name": "haskq",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:landing": "cd apps/landing && npm run dev",
    "dev:docs": "cd apps/docs && npm run dev",
    "dev:playground": "cd apps/playground && npm run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "deploy": "vercel deploy --prod"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "turbo": "^1.10.14",
    "vercel": "^32.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@9.8.1"
} 