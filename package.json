{
  "name": "haskq",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "cd apps/haskq-unified && npm run build",
    "dev": "cd apps/haskq-unified && npm run dev",
    "lint": "cd apps/haskq-unified && npm run lint",
    "clean": "rm -rf node_modules && cd apps/haskq-unified && npm run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "deploy": "vercel deploy --prod"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "vercel": "^32.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@9.8.1"
} 