{
  "version": 2,
  "github": {
    "silent": true
  },
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "framework": "nextjs",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
  "rewrites": [
    { "source": "/playground", "destination": "/playground" },
    { "source": "/playground/(.*)", "destination": "/playground/$1" },
    { "source": "/docs", "destination": "/docs" },
    { "source": "/docs/(.*)", "destination": "/docs/$1" },
    { "source": "/(.*)", "destination": "/$1" }
  ]
} 