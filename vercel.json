{
  "version": 2,
  "builds": [
    {
      "src": "haskq/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/haskq/$1"
    }
  ],
  "github": {
    "silent": true,
    "enabled": true
  }
} 